{
  "name": "@genexusm-plugin-sample/components",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/plugin-components-sample/plugin-components-sample.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "watch": "yarn build.icons && stencil build --watch",
    "build": "yarn build.icons && stencil build",
    "start-stencil": "yarn build.icons && stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "icons-svg": "ssg-svg --srcDir=src/icons/svg-source/ --outDir=src/icons/generated-svg/ --configFilePath=src/icons/config/color-states.json --showcaseDir=src/icons/showcase/ --showcaseBaseHref=../generated-svg/ --logDir=src/icons/log/ --objectFilePath=src/icons/ICONS_ASSETS.ts",
    "icons-sass": "ssg-sass --srcDir=src/icons/generated-svg/ --outDir=src/icons/generated-sass/ --configFilePath=src/icons/config/color-states.json --vendorPrefix=abc",
    "build.icons": "yarn run icons-svg && yarn run icons-sass"
  },
  "dependencies": {
    "@stencil/core": "^4.7.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^21.9.0",
    "@genexus/svg-sass-generator": "1.1.19"
  },
  "license": "MIT"
}
